{
  "name": "home_court",
  "version": "1.0.0",
  "description": "Social application for finding nearby pickup games",
  "main": "index.tsx",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend",
    "debug": "concurrently \"nodemon --inspect app.js\" \"npm run frontend\""
  },
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmbarness/home_court.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mmbarness/home_court/issues"
  },
  "homepage": "https://github.com/mmbarness/home_court#readme",
  "dependencies": {
    "@awaitjs/express": "^0.7.2",
    "@babel/runtime": "^7.14.6",
    "@types/google.maps": "^3.45.6",
    "@types/jwt-decode": "^3.1.0",
    "@types/react": "^17.0.19",
    "@types/react-redux": "^7.1.18",
    "@types/redux": "^3.6.0",
    "@types/underscore": "^1.11.3",
    "aws-sdk": "^2.930.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.6",
    "mongoose": "^5.12.13",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.2",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2",
    "use-places-autocomplete": "^1.9.3",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@types/googlemaps": "^3.43.3"
  }
